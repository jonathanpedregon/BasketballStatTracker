<Window x:Class="BasketballStatTracker.Windows.ReboundWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BasketballStatTracker.Windows"
        mc:Ignorable="d"
        Title="ReboundWindow" Height="450" Width="800" Background="CadetBlue">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="400"></ColumnDefinition>
            <ColumnDefinition Width="400"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Orientation="Vertical" x:Name="Team1StackPanel" Grid.Column="0"></StackPanel>
        <StackPanel Orientation="Vertical" x:Name="Team2StackPanel" Grid.Column="1"></StackPanel>
    </Grid>
</Window>
